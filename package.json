		"@types/react": "^18.3.17",
		"@types/react-dom": "^18.3.5",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "10.4.20",
		"globals": "^15.13.0",
		"husky": "9.1.7",
		"postcss": "8.4.49",
		"tailwindcss": "3.4.17",
		"typescript": "~5.6.2",
		"vite": "^6.0.3"
